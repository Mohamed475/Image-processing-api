{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "simple api endpoint return resized image using the width and height using typescript and jasmine unit testing framework and eslint plus other third-party library",
  "main": "index.js",
  "scripts": {
    "test": "npx tsc && jasmine",
    "dev": "nodemon ./src/index.ts",
    "clean": "rimraf build/",
    "build": "yarn clean && npx tsc",
    "start": "npm run build && nodemon build/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohamed475/Image-processing-api.git"
  },
  "author": "Mohamed Yasser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mohamed475/Image-processing-api/issues"
  },
  "homepage": "https://github.com/Mohamed475/Image-processing-api#readme",
  "dependencies": {
    "express": "^4.17.1",
    "jimp": "^0.16.2"
  },
  "keywords": [],
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "m-zanaty-web-utils": "^0.0.19",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "yarn": "^1.22.19"
  }
}
